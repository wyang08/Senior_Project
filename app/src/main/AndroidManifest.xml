    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Senior_Project">

        <activity android:name=".SplashActivity" android:theme="@style/AppTheme1">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ViewPdf"></activity>

        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>


        <activity android:name=".AdminScreen"/>



        <activity android:name=".Login">

        </activity>





        <activity android:name=".PDFFragment"
            tools:ignore="Instantiatable">
        </activity>

    </application>

</manifest>